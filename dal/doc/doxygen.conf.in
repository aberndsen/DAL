PROJECT_NAME = "Data Access Library (DAL)"

# The files and directories to parse
INPUT = @CMAKE_SOURCE_DIR@/dal @CMAKE_SOURCE_DIR@/dal/hdf5 @CMAKE_SOURCE_DIR@/dal/hdf5/types @CMAKE_SOURCE_DIR@/dal/hdf5/exceptions @CMAKE_SOURCE_DIR@/dal/lofar 
INCLUDE_PATH = @CMAKE_INCLUDE_PATH@

# Where to put the results
OUTPUT_DIRECTORY = @CMAKE_CURRENT_BINARY_DIR@

# Generate XML to use as docstrings in SWIG
GENERATE_XML = YES

# Skip code marked as such ( use #ifndef DOXYGEN_SHOULD_SKIP_THIS )
PREDEFINED = DOXYGEN_SHOULD_SKIP_THIS

# Don't flood us with warnings
# (remove once (almost) all functions are documented
WARN_IF_UNDOCUMENTED = NO

# Don't spam us
QUIET = YES

